{
  "name": "HekrSDK",
  "version": "3.4.1-beta",
  "summary": "SDK for HEKR",
  "description": "A longer description of HekrConfig in Markdown format.\n\n* Think: Why did you write this? What is the focus? What does it do?\n  * CocoaPods will be using this to generate tags, and improve search results.\n  * Try to keep it short, snappy and to the point.\n  * Finally, don't worry about the indent, CocoaPods strips it!",
  "homepage": "http://hekr.me",
  "license": "MIT see http://www.hekr.me",
  "authors": {
    "Mike": "ccteym@gmail.com"
  },
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "git": "https://github.com/HEKR-Cloud/HEKR-IOS-SDK.git",
    "branch": "global"
  },
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "*.{h}",
      "public_header_files": "*.{h}",
      "vendored_libraries": "libHekrSDKCore.a",
      "frameworks": [
        "WebKit"
      ],
      "resource_bundles": {
        "JSSDK": [
          "new.js",
          "old.js"
        ]
      },
      "requires_arc": true,
      "dependencies": {
        "CocoaAsyncSocket": [
          "~> 7.4"
        ],
        "SocketRocket": [
          "~> 0.4"
        ],
        "AFNetworking": [
          "~> 3.0"
        ],
        "ZipArchive": [
          "~> 1.4"
        ],
        "GTSDK": [
          "~> 1.6"
        ],
        "CocoaLumberjack": [
          "~> 2.3.0"
        ],
        "Masonry": [
          "~> 1.0.1"
        ],
        "SHAlertViewBlocks": [
          "~> 1.2.1"
        ]
      }
    },
    {
      "name": "socialWeibo",
      "source_files": [
        "Social/SocialWeibo.m",
        "Social/socialImp.h"
      ],
      "dependencies": {
        "WeiboSDK": [
          "~> 3.1.3"
        ]
      }
    },
    {
      "name": "socialQQ",
      "source_files": [
        "Social/SocialQQ.m",
        "Social/socialImp.h"
      ],
      "dependencies": {
        "TencentOpenApiSDK": [
          "~> 2.9"
        ]
      }
    },
    {
      "name": "socialWeixin",
      "source_files": [
        "Social/SocialWeixin.m",
        "Social/socialImp.h"
      ],
      "dependencies": {
        "WeixinSDK": [
          "~> 1.4"
        ]
      }
    },
    {
      "name": "socialFacebook",
      "source_files": [
        "Social/SocialFacebook.m",
        "Social/socialImp.h"
      ],
      "dependencies": {
        "FBSDKLoginKit": [
          "~> 4.11"
        ]
      }
    },
    {
      "name": "socialTwitter",
      "source_files": [
        "Social/SocialTwitter.m",
        "Social/socialImp.h"
      ],
      "dependencies": {
        "TwitterCore": [
          "~> 1.10.0"
        ],
        "TwitterKit": [
          "~> 1.10.0"
        ]
      }
    },
    {
      "name": "socialGoogle",
      "source_files": [
        "Social/SocialGoogle.m",
        "Social/socialImp.h"
      ],
      "dependencies": {
        "Google/SignIn": [
          "~> 2.0"
        ]
      }
    }
  ]
}
